generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Students {
  kode_siswa    String   @id @unique
  nama_siswa    String
  alamat_siswa  String
  tanggal_siswa DateTime
  jurusan_siswa String
}
